{
  "name": "react-boilerplate",
  "version": "4.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "main": "server/index.js",
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "XXXanalyze:clean": "rimraf stats.json",
    "XXXpreanalyze": "npm run analyze:clean",
    "XXXanalyze": "node ./internals/scripts/analyze.js",
    "XXXextract-intl": "node ./internals/scripts/extract-intl.js",
    "XXXnpmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "XXXpreinstall": "npm run npmcheckversion",
    "XXXprebuild": "npm run build:clean",
    "XXXbuild": "NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "XXXbuild:clean": "rimraf ./build",
    "XXXstart:dev": "NODE_ENV=development node server",
    "start": "node server",
    "XXXstart:tunnel": "NODE_ENV=development ENABLE_TUNNEL=true node server",
    "XXXstart:prod": "NODE_ENV=production node server",
    "XXXpresetup": "npm i chalk shelljs",
    "XXXsetup": "node ./internals/scripts/setup.js",
    "XXXclean": "shjs ./internals/scripts/clean.js",
    "XXXclean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "XXXgenerate": "plop --plopfile internals/generators/index.js",
    "XXXlint": "npm run lint:js && npm run lint:css",
    "XXXlint:css": "stylelint app/**/*.js",
    "XXXlint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "XXXlint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "XXXlint:js": "npm run lint:eslint -- . ",
    "XXXlint:staged": "lint-staged",
    "XXXpretest": "npm run test:clean && npm run lint",
    "XXXtest:clean": "rimraf ./coverage",
    "XXXtest": "NODE_ENV=test jest --coverage",
    "XXXtest:watch": "NODE_ENV=test jest --watchAll",
    "XXXcoveralls": "cat ./coverage/lcov.info | coveralls",
    "XXXprettify": "prettier --write",
    "XXXdocs": "apidoc -i server -o server/docs"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "XXXpre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "compression": "1.7.4",
    "connected-react-router": "6.4.0",
    "cors": "^2.8.5",
    "cross-env": "5.2.0",
    "dotenv-safe": "^8.2.0",
    "email-templates": "^7.1.0",
    "express": "4.16.4",
    "express-validation": "^1.0.2",
    "fontfaceobserver": "2.1.0",
    "google-auth-library": "^6.0.6",
    "googleapis": "^59.0.0",
    "helmet": "^4.1.0",
    "history": "4.9.0",
    "hoist-non-react-statics": "3.3.0",
    "http-status": "^1.4.2",
    "immer": "3.0.0",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "joi": "10.4.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.20",
    "method-override": "^3.0.0",
    "minimist": "^1.2.5",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.10.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-day-picker": "^7.4.8",
    "react-dom": "16.8.6",
    "react-google-login": "^5.1.21",
    "react-helmet": "6.0.0-beta",
    "react-intl": "2.8.0",
    "react-redux": "7.0.2",
    "react-router-dom": "5.0.0",
    "react-with-direction": "^1.3.1",
    "redux": "4.0.1",
    "redux-saga": "1.0.2",
    "reselect": "4.0.0",
    "sanitize.css": "8.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.2.0",
    "sinon": "^9.0.3",
    "styled-components": "4.2.0",
